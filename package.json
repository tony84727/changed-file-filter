{
  "name": "changed-file-filter",
  "version": "0.1.0",
  "private": true,
  "description": "Only run Github Action job when specific files changed",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tony84727/changed-file-filter.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "tony84727",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/exec": "^1.0.4"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/jest-expect-message": "^1.0.2",
    "@types/js-yaml": "^3.12.3",
    "@types/node": "^12.7.12",
    "@types/tmp": "^0.2.0",
    "@typescript-eslint/parser": "^2.8.0",
    "@zeit/ncc": "^0.20.5",
    "eslint": "^5.16.0",
    "eslint-plugin-github": "^2.0.0",
    "eslint-plugin-jest": "^22.21.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "jest-circus": "^26.0.1",
    "jest-expect-message": "^1.0.2",
    "js-yaml": "^3.13.1",
    "lint-staged": "^10.2.2",
    "prettier": "^1.19.1",
    "tmp": "^0.2.1",
    "ts-jest": "^25.5.1",
    "typescript": "^3.6.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  }
}
